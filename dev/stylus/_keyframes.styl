@keyframes fadeIn {
  from {
    opacity: 0
  }
  to {
    opacity: 1
  }
}

@keyframes moveUp {
  0% {
    bottom: 0
  }
  100% {
    bottom: 100%
  }
}

@keyframes moveSideWayRight {
  0% {
    margin-left: 50%
  }
  100% {
    margin-left: 75%
  }
}

@keyframes moveSideWayLeft {
  0% {
    margin-left: 15%
  }
  100% {
    margin-left: 45%
  }
}